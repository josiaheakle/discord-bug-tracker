{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"rootDir": "src",
		"outDir": "dist",
		"removeComments": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"strictPropertyInitialization": false,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictNullChecks": true,
		"skipLibCheck": true
	}
}
